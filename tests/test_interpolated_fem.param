% parameters for program test_link_fem                                    %
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%% pde parameters :	        				      %%%%%
N = 2;                  % dimension of the domain.
LX = 1.0;		% size in X.
LY = 1.0;	        % size in Y.
LZ = 1.0;		% size in Z.

%%%%%   discretisation parameters  :                     	      %%%%%
K = 1;                  % Finite element degree.
KI = 2;                 % Integration degree
INTEGRATION = 17;       % 0 = exact integration.
			% 1 = Newton Cotes of degree KI
			% 2 = Gauss for segment of degree KI
			% 3 = composite with Newton cotes, order 2*K, KI decomp
			% 11, 12, 13, 14, 15, 16, 17 triangle(n-10)
			% 21, 22, 23, 25 tetrahedron(n-20)
NX1 = 5;            	% space step for mesh1.
NX2 = 7;            	% space step for mesh2.

